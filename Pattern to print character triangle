import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int size = sc.nextInt();
        char a = 'A';
        for (int row = 0; row < size; row++) {
            for (int col = 0; col <= row; col++) {
                
                System.out.print(a);
            }
            a++;
            System.out.println();
        }
    }
}
