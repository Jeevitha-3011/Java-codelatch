import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int a=sc.nextInt();
	    for(int row=0;row<a;row++){
	        for(int col=0;col<a;col++){
	            System.out.print("*");
	        }
	        System.out.println();
	    }
	    
	}
}
